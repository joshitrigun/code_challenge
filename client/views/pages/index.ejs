<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head'); %>
    </head>
    <body class="container">
        <main>
            <% if (users) { %>
            <h2>Users <span class="h4"><%= users.length %></span></h2>
            <% users.forEach(function(user) { %>
            <div class="card">
                <div class="card-body">
                    <p>ID: <%= user.id %></p>
                    <p>Name: <%= user.name %></p>
                    <p>Age: <%= user.age %></p>
                    <p>Date: <%= user.date %></p>
                </div>
            </div>
            <% }); %> <%} else { %>
            <p>User detail not found</p>
            <% } %>
        </main>
    </body>
</html>
